{
    "version": 2,
    "rewrites": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "runtime": "python3.11",
            "memory": 1024,
            "maxDuration": 10
        }
    }
}